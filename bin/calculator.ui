<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkWindow" id="calculator_window">
    <property name="title">Калькулятор Final</property>
    <property name="default-width">300</property>
    <property name="default-height">400</property>
    <property name="resizable">false</property>

    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">5</property>
        <property name="margin-start">10</property>
        <property name="margin-end">10</property>
        <property name="margin-top">10</property>
        <property name="margin-bottom">10</property>

        <child>
          <object class="GtkEntry" id="display_entry">
            <property name="halign">fill</property>
            <property name="editable">false</property>
            <property name="text">0</property>
            <property name="xalign">1.0</property>
          </object>
        </child>

        <child>
          <object class="GtkGrid">
            <property name="row-spacing">5</property>
            <property name="column-spacing">5</property>
            <property name="halign">fill</property>

            <child>
              <object class="GtkButton" id="button_clear">
                <property name="label">C</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_plus_minus">
                <property name="label">+/-</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_percent">
                <property name="label">%</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_divide">
                <property name="label">/</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>

            <child>
              <object class="GtkButton" id="button_seven">
                <property name="label">7</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_eight">
                <property name="label">8</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_nine">
                <property name="label">9</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_multiply">
                <property name="label">X</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>

            <child>
              <object class="GtkButton" id="button_four">
                <property name="label">4</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_five">
                <property name="label">5</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_six">
                <property name="label">6</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_minus">
                <property name="label">-</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>

            <child>
              <object class="GtkButton" id="button_one">
                <property name="label">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_two">
                <property name="label">2</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_three">
                <property name="label">3</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_plus">
                <property name="label">+</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>

            <child>
              <object class="GtkButton" id="button_zero">
                <property name="label">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">4</property>
                  <property name="width">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_dot">
                <property name="label">.</property>
                <layout>
                  <property name="column">2</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_calculate">
                <property name="label">=</property>
                <layout>
                  <property name="column">3</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
